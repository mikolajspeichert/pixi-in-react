{
  "name": "react-pixi-fiber",
  "version": "0.6.1",
  "description": "React Fiber renderer for PixiJS",
  "main": "index.js",
  "author": "Michal Ochman",
  "license": "MIT",
  "repository": "github:michalochman/react-pixi-fiber",
  "typings": "./index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "index.d.ts",
    "index.js",
    "react-pixi-alias.js",
    "cjs/",
    "src/"
  ],
  "dependencies": {
    "fbjs": "^1.0.0",
    "performance-now": "^2.1.0",
    "react-reconciler": "^0.17.2"
  },
  "peerDependencies": {
    "pixi.js": "^4.8.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@types/pixi.js": "^4.8.3",
    "@types/react": "^16.7.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-rewire": "^1.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "canvas-prebuilt": "^2.0.0-alpha.14",
    "codecov": "^3.1.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-css-modules": "^2.10.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "pixi.js": "^4.8.2",
    "prettier": "^1.15.2",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-test-renderer": "^16.6.3",
    "rollup": "^0.67.1",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-plugin-visualizer": "^0.9.2",
    "typescript": "^3.1.6"
  },
  "scripts": {
    "build": "npm run build:prod && npm run build:dev",
    "build:dev": "NODE_ENV=development rollup -c config/rollup.config.js",
    "build:prod": "NODE_ENV=production rollup -c config/rollup.config.js",
    "build:alias:dev": "NODE_ENV=development rollup -c config/rollup.config.js --environment entry:alias",
    "build:alias:prod": "NODE_ENV=production rollup -c config/rollup.config.js --environment entry:alias",
    "build:index:dev": "NODE_ENV=development rollup -c config/rollup.config.js --environment entry:index",
    "build:index:prod": "NODE_ENV=production rollup -c config/rollup.config.js --environment entry:index",
    "eslint": "eslint src",
    "eslint-check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
    "prepublishOnly": "npm run test && npm run build",
    "test": "jest",
    "test:ts": "tsc -p tsconfig.json"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "globals": {
      "__DEV__": true
    },
    "setupFiles": [
      "./test/jest.setupPixi.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
